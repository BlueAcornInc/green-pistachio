"//
// @package     BlueAcorn/site
// @author      Blue Acorn iCi <code@blueacorn.com>
// @copyright   Copyright © Blue Acorn iCi. All rights reserved.
//

@sprite-url: \\"../images/sprites.svg\\";
@sprite-width: 121px;
@sprite-height: 17px;
@svg-icon-stars-x: 0;
@svg-icon-stars-y: 0;
@svg-icon-stars-xy: 0 0;
@svg-icon-stars-width: 121px;
@svg-icon-stars-height: 17px;

.svg() {
    background-image: url(\\"@{sprite-url}\\");
    background-repeat: no-repeat;
}

[class*=\\"svg-icon-\\"] {
    display: inline-block;
}

.svg-icon-stars() {
    background-position: @svg-icon-stars-xy;
}

.svg-icon-stars-dims() {
    width: @svg-icon-stars-width;
    height: @svg-icon-stars-height;
}

.svgi-icon-stars() {
    .svg();
    .svg-icon-stars();
    .svg-icon-stars-dims();
}

& when (@media-common = true) {
    .svg-icon-icon-stars {
        .svgi-icon-stars();
    }
}
"
`;

